<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="vboxSubmissoes" prefHeight="655.0" prefWidth="180.0" stylesheets="@../css/Vbars.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.fatec.bancodebravos.schedu.controller.VboxSubmissoesController">
   <children>
      <VBox style="-fx-background-color: black;" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Button fx:id="btnSubEntregas" mnemonicParsing="false" onAction="#painelSubEntregas" prefHeight="60.0" prefWidth="160.0" styleClass="botoes" text="Professores">
               <VBox.margin>
                  <Insets right="20.0" top="120.0" />
               </VBox.margin>
               <graphic>
                  <ImageView fitHeight="36.0" fitWidth="36.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../imagens/entrega.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <ImageView fitHeight="120.0" fitWidth="180.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../imagens/logo.png" />
               </image>
               <VBox.margin>
                  <Insets top="60.0" />
               </VBox.margin>
            </ImageView>
            <Button fx:id="btnSubDevolutivas" mnemonicParsing="false" onAction="#painelSubDevolutivas" prefHeight="60.0" prefWidth="160.0" styleClass="botoes" text="Disciplinas">
               <VBox.margin>
                  <Insets right="20.0" top="60.0" />
               </VBox.margin>
               <graphic>
                  <ImageView fitHeight="32.0" fitWidth="32.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../imagens/devolutiva.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button fx:id="btnSubNotas" mnemonicParsing="false" onAction="#painelSubNotas" prefHeight="60.0" prefWidth="160.0" styleClass="botoes" text="Cursos">
               <graphic>
                  <ImageView fitHeight="36.0" fitWidth="36.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../imagens/notas.png" />
                     </image>
                  </ImageView>
               </graphic>
               <VBox.margin>
                  <Insets right="20.0" top="60.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
